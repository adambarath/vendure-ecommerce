{
  "name": "store",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev:server": "ts-node ./src/index.ts",
    "dev:worker": "ts-node ./src/index-worker.ts",
    "dev": "concurrently npm:dev:*",
    "build": "tsc",
    "start:server": "node ./dist/index.js",
    "start:worker": "node ./dist/index-worker.js",
    "start": "concurrently npm:start:*",
    "populate": "ts-node ./src/utils/populate-test-data.ts",
    "codegen": "graphql-codegen --config ./_codegen.ts"
  },
  "dependencies": {
    "@aws-sdk/client-ses": "^3.662.0",
    "@vendure/admin-ui-plugin": "3.0.3",
    "@vendure/asset-server-plugin": "3.0.3",
    "@vendure/core": "3.0.3",
    "@vendure/create": "^3.0.3",
    "@vendure/email-plugin": "3.0.3",
    "@vendure/payments-plugin": "^3.0.3",
    "better-sqlite3": "9.6.0",
    "dotenv": "16.4.5",
    "pg": "^8.13.0",
    "stripe": "^13.11.0"
  },
  "devDependencies": {
    "@vendure/cli": "^3.0.3",
    "concurrently": "9.0.1",
    "typescript": "5.3.3"
  }
}
